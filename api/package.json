{
  "name": "avenue-api",
  "author": "dscnitrourkela",
  "description": "GraphQL APIs for Avenue NITR",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "homepage": "https://github.com/dscnitrourkela/project-dates#readme",
  "keywords": [
    "nitr",
    "avenue",
    "dsc",
    "dscnitr",
    "api",
    "graphql",
    "apollo",
    "typescript",
    "node",
    "mongodb",
    "prisma",
    "nexus"
  ],
  "bugs": {
    "url": "https://github.com/dscnitrourkela/project-dates/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dscnitrourkela/project-dates.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "dev2": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "generate": "ts-node --transpile-only src/config/nexus",
    "prettier:fix": "yarn prettier --write .",
    "prettier:check": "yarn prettier --check .",
    "lint:fix": "yarn eslint --fix \"**/*.ts\"",
    "lint:find": "yarn eslint \"**/*.tx\""
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@apollo/server": "^4.0.0",
    "apollo-server": "^3.10.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "nexus": "^1.3.0",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2"
  }
}
